<p id="notice"><%= notice %></p>
<table>
  <tr>
    <th>Team</th>
    <th colspan="3"></th>
  </tr>
  <% @teams.each do |team| %>
    <tr>
      <td><%= team.name %></td>
      <td><%= link_to 'Show Roles', team %></td>
      <td><%= link_to 'Edit', edit_team_path(team) %></td>
      <td><%= link_to 'Delete', team, method: :delete, data: {confirm: 'Delete?'} %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Add', new_team_path %>